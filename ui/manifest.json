            mgr.addResource('modern75/sencha-modern-overrides.css');
            mgr.addResource('modern75/framework/packages/ux/modern-neptune/resources/ux-all.css');
            mgr.addResource('modern75/sencha-modern-prepare.js');
            mgr.addResource('modern75/framework/ext-modern-all-debug.js');
            mgr.addResource('modern75/framework/packages/ux/ux-debug.js');            
            mgr.addResource('modern75/framework/packages/charts/charts-debug.js');
            mgr.addResource('modern75/framework/packages/google/google-debug.js');
            mgr.addResource('modern75/framework/packages/exporter/exporter-debug.js');
            mgr.addResource('modern75/framework/packages/calendar/calendar-debug.js');
            mgr.addResource('modern75/framework/packages/pivot/pivot-debug.js');
            mgr.addResource('modern75/framework/packages/d3/d3-debug.js');
