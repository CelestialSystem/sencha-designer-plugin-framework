{
    "name": "ext75",
    "version": "0.1",
    "author": "Sencha Inc.",
    "description": "Ext JS 7.5.x Framework",
    "plugins": [
        "~/app",
        "~/component",
        "~/definitions",
        "~/ui"
    ],
    "files": [
        "../../../src/frameworks/ext75/Environment.js",
        "../../../src/frameworks/ext75/DefaultStores.js"
    ],
    "destroy": function() {
        var plugins = ["Ext JS 7.5.x Framework - UI", "Ext JS 7.5.x Framework - Component", "Ext JS 7.5.x - Definitions", "Ext JS 7.5.x Framework - App"];
        plugins.forEach(function(plugin) {
            sencha.plugin.Manager.unload(plugin);
        });
        delete xds.builtInSnapshots;
        xds.builtInSnapshots = {};
        xds.component.Registry.clear();
    }
}
